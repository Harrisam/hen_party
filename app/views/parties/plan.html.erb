<h1>Plan <%= @party.name %></h1>

<div class='chart'>
	<% @party.participants.each do |f|%>
		<% f.response.budgets.each do |b| %>
			<div style="width: 100px;"><%= b.amount %></div>	
	<% f.response.date_options.each do |b| %>
		<p><%= b.start_date %> - <%= b.start_date %><p>	
	<% end %>
	<% end %>
	<%end %>
</p>



	


    


<p><%= link_to 'See party details', party_path(@party), class: 'btn btn-default btn-lg' %></p>


